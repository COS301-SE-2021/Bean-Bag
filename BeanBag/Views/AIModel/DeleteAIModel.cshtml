@model AIModel
@using Microsoft.Extensions.Options
@using Microsoft.Identity.Web
@inject IOptions<MicrosoftIdentityOptions> _azureAdb2COptions
@{
    Layout = null;
}

<div class="modal-body">
    <div class="form-group">
        <div class="text-muted">Are you sure you want to delete this AI Model ?</div>
       
     
               <div class="stats mt-2 pt-4">
                    <div class="d-flex justify-content-between p-price"><span>Name</span><span>@Model.name</span></div>
                    <div class="d-flex justify-content-between p-price"><span>ID</span><span>@Model.Id</span></div>
                    <div class="d-flex justify-content-between p-price"><span>Date Created</span><span>@Model.dateCreated</span></div>
                    <div class="d-flex justify-content-between p-price"><span>Description</span><span>@Model.description</span></div>
                </div>
        <input asp-for="Id" hidden name="projectId"/>
        <div asp-validation-summary="All" class="text-danger"></div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
    <input type="submit" class="btn btn-3" value="Delete" />
</div>