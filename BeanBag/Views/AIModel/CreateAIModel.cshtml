@model CreateNewAIModel
@using Microsoft.Extensions.Options
@using Microsoft.Identity.Web
@inject IOptions<MicrosoftIdentityOptions> _azureAdb2COptions

@{
    var options = _azureAdb2COptions.Value;
}

<link rel="stylesheet" type="text/css" href="css\dashboard.css" />
<!-- From Reno: Note the name of the class for the below section. Change if you must :) -->
<section class="inventory d-flex justify-content-center">

    <form method="post" enctype="multipart/form-data" asp-action="newAIModel" class="createForm">
        <div class="form-group row">
            <label>AI Model Name:</label>
            <input class="form-control" asp-for="name"/>
        </div>
        <div class="form-group row">
            <label>Upload Images</label>
            <input asp-for="image" type="file" class="form-control" />
        </div>
        <input type="submit" class="btn btn-1" value="Create" />
        <a asp-action="Index" class="btn btn-2">Back</a>
</form>

</section>